<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_height="match_parent" android:layout_width="match_parent">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp">



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp">



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#31383E">




                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/text1"
                    android:textSize="16dp"
                    android:paddingLeft="3dp"
                    android:textColor="@android:color/white"
                    android:text="SLNO."/>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight=".8">




                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/text2"
                    android:textSize="16dp"
                    android:paddingLeft="3dp"
                    android:textStyle="bold"
                    android:text="AGENT"
                    android:layout_gravity="center"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#31383E">




                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/text3"
                    android:textSize="16dp"
                    android:paddingLeft="3dp"
                    android:layout_gravity="center"
                    android:textColor="@android:color/white"
                    android:text="PHONE"/>

            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/text4"
                    android:textSize="15dp"
                    android:layout_gravity="center"
                    android:paddingLeft="3dp"
                    android:textStyle="bold"
                    android:text="EMAIL"/>

            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#31383E">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/text5"
                    android:textSize="15dp"
                    android:paddingLeft="3dp"
                    android:layout_gravity="center"
                    android:textColor="@android:color/white"
                    android:text="20D"/>
            </LinearLayout>





            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/text6"
                    android:textSize="15dp"
                    android:paddingLeft="3dp"
                    android:textStyle="bold"
                    android:layout_gravity="center"
                    android:text="20R"/>
            </LinearLayout>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#31383E">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/text7"
                    android:textSize="15dp"
                    android:paddingLeft="3dp"
                    android:textColor="@android:color/white"
                    android:text="40D"
                    android:layout_gravity="center"/>
            </LinearLayout>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/text8"
                    android:textSize="15dp"
                    android:paddingLeft="3dp"
                    android:text="40R"
                    android:textStyle="bold"
                    android:layout_gravity="center"/>
            </LinearLayout>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#31383E">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/text9"
                    android:textSize="15dp"
                    android:paddingLeft="3dp"
                    android:textColor="@android:color/white"
                    android:text="T-D"
                    android:layout_gravity="center"/>

            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/text10"
                    android:textSize="15dp"
                    android:paddingLeft="3dp"
                    android:text="T-R"
                    android:textStyle="bold"
                    android:layout_gravity="center"/>



            </LinearLayout>



        </LinearLayout>



    </LinearLayout>



</LinearLayout>